<?xml version="1.0" encoding="UTF-8"?>
<!-- For LineageOS 20 (Android 13) -->
<compatibility-matrix version="7" type="device">
    
    <!-- Add all vendor HALs from your manifest -->
    <hal format="hidl" optional="true">
        <name>vendor.oppo.engnative.engineer</name>
        <version>1.0</version>
        <interface>
            <name>IEngineer</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="hidl" optional="true">
        <name>vendor.sprd.hardware.connmgr</name>
        <version>1.0</version>
        <interface>
            <name>IConnmgr</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>vendor.sprd.hardware.enhance</name>
        <version>1</version>
        <interface>
            <name>IEnhance</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="hidl" optional="true">
        <name>vendor.sprd.hardware.network</name>
        <version>1.0</version>
        <interface>
            <name>INetworkControl</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="hidl" optional="true">
        <name>vendor.sprd.hardware.wcn</name>
        <version>1.0</version>
        <interface>
            <name>IWcnVendorInterface</name>
            <instance>default</instance>
        </interface>
    </hal>
    
    <!-- CORRECT sepolicy version for Android 13 -->
    <sepolicy>
        <version>33.0</version>
    </sepolicy>
    
    <!-- Kernel version -->
    <kernel version="6.0"/>
</compatibility-matrix>